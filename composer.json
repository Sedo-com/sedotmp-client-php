{
    "name": "sedo-com/sedotmp-client-php",
    "version": "0.0.1",
    "description": "",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/Sedo-com/sedotmp-client-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Conrad Barthelmes",
            "homepage": "https://github.com/bartrail"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "auth0/auth0-php": "^8.0",
        "symfony/dotenv": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": { "Sedo\\" : "./lib/src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sedo\\" : "test/" }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
